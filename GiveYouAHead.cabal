author: Qinka <qinka@live.com>
tested-with: GHC ==7.10.1

library
    exposed-modules:
        Data.GiveYouAHead
        Data.GiveYouAHead.Json
        Data.GiveYouAHead.Json.DelList
        Date.GiveYouAHead
        GiveYouAHead.Init
        Localization.GiveYouAHead
        Localization.GiveYouAHead.Test
    exposed: False
    build-depends:
        base >4.7
    default-language: Haskell2010
    hs-source-dirs: src

test-suite test-GiveYouAHead
